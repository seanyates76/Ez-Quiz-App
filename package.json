{
  "name": "ez-quiz-app",
  "version": "3.4.0",
  "private": true,
  "license": "MIT",
  "description": "Static PWA + Netlify Functions for EZ Quiz",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest -i --passWithNoTests",
    "ui:check": "node ./scripts/ui-check.mjs",
    "lint": "eslint . || true",
    "lint:fix": "eslint . --fix || true",
    "fmt:check": "prettier -c . || true",
    "fmt:write": "prettier -w . || true",
    "test:parser": "node ./tests/parser.corpus.mjs || echo Parser corpus missing",
    "reiterate": "node ../ezq-dev-tools/reiterate.mjs"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "netlify-cli": "^23.9.5",
    "nodemailer": "^7.0.10"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^27.0.0",
    "puppeteer": "^24.27.0"
  }
}
